@model Kancelaria.Models.Case

<div class="row">
    <div class="col-lg-5" style="float:left">
        <div class="form-inline" style="margin-bottom:20px">
            <label asp-for="repetytorium.CassationCourtName"></label>
            <input asp-for="repetytorium.CassationCourtName" class="form-control input-20" />
            <span asp-validation-for="repetytorium.CassationCourtName" class="text-danger" />
        </div>
        <table>
            <tr>
                <td>
                    <div>
                        <label asp-for="repetytorium.CassationDataWyslaniaApelacji"></label>
                        <input asp-for="repetytorium.CassationDataWyslaniaApelacji" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.CassationDataWyslaniaApelacji" class="text-danger" />
                    </div>
                </td>
                <td>
                    <div>
                        <label asp-for="repetytorium.CassationDataOdbioruApelacji"></label>
                        <input asp-for="repetytorium.CassationDataOdbioruApelacji" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.CassationDataOdbioruApelacji" class="text-danger" />
                    </div>
                </td>
            </tr>
        </table>

        <div>
            <label asp-for="repetytorium.CassationDataWyrokuApelacyjnego"></label>
            <input asp-for="repetytorium.CassationDataWyrokuApelacyjnego" class="form-control input-20" />
            <span asp-validation-for="repetytorium.CassationDataWyrokuApelacyjnego" class="text-danger" />
        </div>
        <table>
            <tr>
                <td>
                    <div>
                        <label asp-for="repetytorium.CassationWysokoscOplaty"></label>
                        <input asp-for="repetytorium.CassationWysokoscOplaty" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.CassationWysokoscOplaty" class="text-danger" />
                    </div>
                </td>
                <td>
                    <div>
                        <label asp-for="repetytorium.CassationDataOplaty"></label>
                        <input asp-for="repetytorium.CassationDataOplaty" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.CassationDataOplaty" class="text-danger" />
                    </div>

                </td>
            </tr>
        </table>

        <fieldset class="form-group">
            <legend style="margin-bottom:10px; margin-top:20px; margin-left:20px">Opłaty dokonał:</legend>
            <div class="row" style="background-color:lightgray">
                <table>
                    <tr>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.CassationOplatyDokonal" type="radio" class="form-check-input" value="0">
                                    Klient
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.CassationOplatyDokonal" type="radio" class="form-check-input" value="1">
                                    Kancelaria
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.CassationOplatyDokonal" type="radio" class="form-check-input" value="2">
                                    Zleceniodawca
                                </label>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        </fieldset>

        <!--END-->

    </div>
    <div class="col-lg-5" style="float:right">
        <div class="form-inline">
            <label class="form-check-label" style="margin-bottom:30px">
                <input asp-for="repetytorium.CassationPrawomocnosc" type="checkbox" class="form-check-input">
                Stwierdzona prawomocność orzeczenia
            </label>
            <span asp-validation-for="repetytorium.CassationPrawomocnosc" class="text-danger" />
        </div>

        <div class="form-inline">
            <label asp-for="repetytorium.CassationKosztyWnioskowane" class="input-20"></label>
            <input asp-for="repetytorium.CassationKosztyWnioskowane" class="form-control input-20" />
            <span asp-validation-for="repetytorium.CassationKosztyWnioskowane" class="text-danger" />
        </div>
        <div class="form-inline">
            <label asp-for="repetytorium.CassationKosztyPrzyznane" class="input-20"></label>
            <input asp-for="repetytorium.CassationKosztyPrzyznane" class="form-control input-20" />
            <span asp-validation-for="repetytorium.CassationKosztyPrzyznane" class="text-danger" />
        </div>
    </div>

</div>


@model Kancelaria.Models.Case

<div class="row">
    <div class="col-lg-5" style="float:left">
        <div class="form-inline" style="margin-bottom:20px">
            <label asp-for="repetytorium.JudgmentCourtName"></label>
            <input asp-for="repetytorium.JudgmentCourtName" class="form-control input-20" />
            <span asp-validation-for="repetytorium.JudgmentCourtName" class="text-danger" />
        </div>
        <table>
            <tr>
                <td>
                    <div class="form-inline">
                        <label asp-for="repetytorium.JudgmentDataWydaniaNakazuZaplaty" class="input-25"></label>
                        <input asp-for="repetytorium.JudgmentDataWydaniaNakazuZaplaty" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.JudgmentDataWydaniaNakazuZaplaty" class="text-danger" />

                    </div>
                </td>
                <td>

                    <div class="form-inline" style="background-color:lightgray">
                        <fieldset class="form-inline">
                            <table>
                                <tr>
                                    <td>
                                        <label class="form-check-label">
                                            <input asp-for="repetytorium.JudgmentNakazZaplaty" type="radio" class="form-check-input" value="0">
                                            Nakazowe
                                        </label>
                                    </td>
                                    <td>
                                        <label class="form-check-label" style="margin-left:20px">
                                            <input asp-for="repetytorium.JudgmentNakazZaplaty" type="radio" class="form-check-input" value="1">
                                            Upominawcze
                                        </label>
                                    </td>
                                </tr>
                            </table>
                        </fieldset>
                    </div>
                </td>
            </tr>
        </table>
        <div>
            <label asp-for="repetytorium.JudgmentDataOdbioruNakazuZaplaty"></label>
            <input asp-for="repetytorium.JudgmentDataOdbioruNakazuZaplaty" class="form-control input-20" />
            <span asp-validation-for="repetytorium.JudgmentDataOdbioruNakazuZaplaty" class="text-danger" />
        </div>
        <fieldset class="form-group" style="margin-top:30px; margin-left:20px">
            <div class="row" style="background-color:lightgray">
                <table>
                    <tr>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.JudgmentOrzeczenie" type="radio" class="form-check-input" value="0">
                                    Zarzuty
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.JudgmentOrzeczenie" type="radio" class="form-check-input" value="1">
                                    Sprzeciw
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.JudgmentOrzeczenie" type="radio" class="form-check-input" value="2">
                                    Wyrok
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.JudgmentOrzeczenie" type="radio" class="form-check-input" value="3">
                                    Wyrok zaoczny
                                </label>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        </fieldset>
    </div>

    <div class="col-lg-5" style="float:right">
        <div class="form-check">
            <label class="form-check-label" style="margin-bottom:30px">
                <input asp-for="repetytorium.JudgmentPrawomocnosc" type="checkbox" class="form-check-input">
                Stwierdzona prawomocność orzeczenia
            </label>
            <span asp-validation-for="repetytorium.JudgmentPrawomocnosc" class="text-danger" />
        </div>

    </div>
</div>

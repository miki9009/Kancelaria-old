                        <span asp-validation-for="repetytorium.SummonsTime1" class="text-danger" />
                    </div>
                </div>
            </div>
            <h4 class="title" style="text-align:center; background-color: white; color:black"><b>UWAGI</b></h4>
            <textarea asp-for="repetytorium.RemarkSummons1" class="form-control" id="exampleTextarea" rows="4"></textarea>
            <span asp-validation-for="repetytorium.RemarkSummons1" class="text-danger" />

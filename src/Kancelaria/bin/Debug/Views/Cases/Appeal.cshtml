@model Kancelaria.Models.Case

<div class="row">
    <div class="col-lg-5" style="float:left">
        <div class="form-inline" style="margin-bottom:20px">
            <label asp-for="repetytorium.AppealCourtName"></label>
            <input asp-for="repetytorium.AppealCourtName" class="form-control input-20" />
            <span asp-validation-for="repetytorium.AppealCourtName" class="text-danger" />
        </div>
        <table>
            <tr>
                <td>
                    <div>
                        <label asp-for="repetytorium.AppealDataWyslaniaApelacji"></label>
                        <input asp-for="repetytorium.AppealDataWyslaniaApelacji" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.AppealDataWyslaniaApelacji" class="text-danger" />
                    </div>
                </td>
                <td>
                    <div>
                        <label asp-for="repetytorium.AppealDataOdbioruApelacji"></label>
                        <input asp-for="repetytorium.AppealDataOdbioruApelacji" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.AppealDataOdbioruApelacji" class="text-danger" />
                    </div>
                </td>
            </tr>
        </table>

        <div>
            <label asp-for="repetytorium.AppealDataWyrokuApelacyjnego"></label>
            <input asp-for="repetytorium.AppealDataWyrokuApelacyjnego" class="form-control input-20" />
            <span asp-validation-for="repetytorium.AppealDataWyrokuApelacyjnego" class="text-danger" />
        </div>
        <table>
            <tr>
                <td>
                    <div>
                        <label asp-for="repetytorium.AppealWysokoscOplaty"></label>
                        <input asp-for="repetytorium.AppealWysokoscOplaty" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.AppealWysokoscOplaty" class="text-danger" />
                    </div>
                </td>
                <td>
                    <div>
                        <label asp-for="repetytorium.AppealDataOplaty"></label>
                        <input asp-for="repetytorium.AppealDataOplaty" class="form-control input-20" />
                        <span asp-validation-for="repetytorium.AppealDataOplaty" class="text-danger" />
                    </div>

                </td>
            </tr>
        </table>

        <fieldset class="form-group" style="margin-left:20px">
            <legend style="margin-bottom:10px; margin-top:20px;">Opłaty dokonał:</legend>
            <div class="row" style="background-color:lightgray">
                <table>
                    <tr>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.AppealOplatyDokonal" type="radio" class="form-check-input" value="0">
                                    Klient
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check" style="margin-right:30px;">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.AppealOplatyDokonal" type="radio" class="form-check-input" value="1">
                                    Kancelaria
                                </label>
                            </div>
                        </td>
                        <td>
                            <div class="form-check">
                                <label class="form-check-label">
                                    <input asp-for="repetytorium.AppealOplatyDokonal" type="radio" class="form-check-input" value="2">
                                    Zleceniodawca
                                </label>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        </fieldset>

        <!--END-->

    </div>
    <div class="col-lg-5" style="float:right">
        <div class="form-check">
            <label class="form-check-label" style="margin-bottom:30px">
                <input asp-for="repetytorium.AppealPrawomocnosc" type="checkbox" class="form-check-input">
                Stwierdzona prawomocność orzeczenia
            </label>
            <span asp-validation-for="repetytorium.AppealPrawomocnosc" class="text-danger" />
        </div>

        <div class="form-inline">
            <label asp-for="repetytorium.AppealKosztyWnioskowane" class="input-20"></label>
            <input asp-for="repetytorium.AppealKosztyWnioskowane" class="form-control input-20" />
            <span asp-validation-for="repetytorium.AppealKosztyWnioskowane" class="text-danger" />
        </div>
        <div class="form-inline">
            <label asp-for="repetytorium.AppealKosztyPrzyznane" class="input-20"></label>
            <input asp-for="repetytorium.AppealKosztyPrzyznane" class="form-control input-20" />
            <span asp-validation-for="repetytorium.AppealKosztyPrzyznane" class="text-danger" />
        </div>
    </div>

</div>

